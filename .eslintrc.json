{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true
  },
  "extends":  [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "rules":  {
    "max-len": ["error", {"code": 100, "ignoreUrls": true}],
    "prettier/prettier": ["error", {"printWidth": 100, "singleQuote": true}]
  }
}
