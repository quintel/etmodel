%h1 All output elements

%h2 Temporarily disabled

-#
  - @output_elements.each do |o|
    .chart
      %h3
        = o.key
        = link_to 'show', [:admin, o]
      = content_tag_for :div, o do
        Loading
      :javascript
        charts.load(#{o.id}, "output_element_#{o.id}", true);

  - content_for :head do
    = render "layouts/etm/javascript_globals"
    = javascript_include_tag 'etm'
  - content_for :footer do
    :javascript
      App.settings = new Setting(#{Current.setting.to_json.html_safe});
      App.call_api()
