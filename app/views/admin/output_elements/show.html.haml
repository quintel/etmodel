%h1= @output_element.name

%table{table_defaults,:style=>"float:left"}
  %thead
    %tr
      %th.tal id
      %th.tal label
      %th.tal key
      %th.tal color
      %th.tal is_target

  %tbody
    -@output_element.output_element_series.each do |serie|
      %tr{cycles}
        %td= serie.id
        %td= link_to serie.label, admin_output_element_serie_path(serie)
        %td= truncate(serie.key, :length => 50)
        %td= serie.color
        %td= serie.is_target
%br        
%p= link_to 'Create new serie', new_admin_output_element_serie_path
- charts