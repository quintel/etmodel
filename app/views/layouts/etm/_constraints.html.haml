#constraints_container
  #constraints
    = render 'layouts/etm/constraint_items', :constraints => @constraints
    = link_to 'Change', '#', :id => 'dashboard_change'
    .clear

    %script
      - @constraints.each do |c|
        dashboard.add(#{ c.to_json.html_safe });

      - Current.view.targets.each do |t|
        new Target(#{t.js_attributes.to_json.html_safe});
