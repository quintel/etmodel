#active-content
  = render "layouts/etm/title"
  #accordion_wrapper
    - if @slides.any?
      %ul.accordion
        = render :partial => 'tab/slide', :collection => @slides
    - else
      = t("not_available")

  #charts_wrapper
    = render :partial => "output_elements/chart",
      :locals => {:chart => @output_element, :dom_id => 'current_chart'}
    =# render :partial => "output_elements/chart",
      :locals => {:chart => nil, :dom_id => 'alternate_chart'}

- content_for :footer do
  = render 'output_elements/table_template'
