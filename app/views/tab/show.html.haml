#active-content
  = render "layouts/etm/title"
  #accordion_wrapper
    - slides = @interface.slides.reject(&:not_allowed_in_this_area)
    - if slides.any?
      %ul.accordion
        = render :partial => 'tab/slide', :collection => slides
    - else
      = t("not_available")

  #charts_wrapper

  .clear

- content_for :scripts do
  = render 'output_elements/table_template'
  = render 'output_elements/chart_template'
