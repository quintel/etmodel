#active-content
  = render "layouts/etm/title"
  #accordion_wrapper
    - slides = @interface.slides.reject(&:not_allowed_in_this_area)
    - if slides.any?
      %ul.accordion
        = render :partial => 'tab/slide', :collection => slides
    - else
      = t("not_available")

  #charts_wrapper
    - Current.setting.charts.each_pair do |holder_id, data|
      = render :partial => "output_elements/chart",
        :locals => {:dom_id => holder_id, :data => data}

  .clear

- content_for :scripts do
  = render 'output_elements/table_template'
